            <option value=<%= folder %> </option><%= folder %> </option> <% }); %>
        </select>
        <select name="puce" id="puce">
            <% for(var i=1; i<17; i++){ %>
            <option value="puce<%= i %>">capteur <%= i %> </option>
            <% } %> 
        </select>
        <input type="submit" value="OK">
    </form>

